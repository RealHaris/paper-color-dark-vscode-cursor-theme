  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#d787ff"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#5fff5f"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#5fff5f"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ffaf5f"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#ff5f5f"
      }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#d0d0d0"
      }
    },
    {
      "scope": ["variable.language", "variable.parameter.function"],
      "settings": {
        "foreground": "#d787ff"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#87d7ff"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#ffaf5f"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#87d7ff"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#d0d0d0"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#d0d0d0"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#5fff5f"
      }
    },
    {
      "scope": ["punctuation.definition.variable", "punctuation.definition.parameters"],
      "settings": {
        "foreground": "#d787ff"
      }
    },
    {
      "scope": ["support.constant.property-value", "support.constant.font-name"],
      "settings": {
        "foreground": "#ff5f5f"
      }
    },
    {
      "scope": ["meta.preprocessor"],
      "settings": {
        "foreground": "#ffaf5f"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#87d7ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#87d7ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.raw", "markup.inline.raw"],
      "settings": {
        "foreground": "#5fff5f"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#ff5f5f"
      }
    }
  ]
}
